# 君悦彩虹SPA前端生产环境配置

# API配置
VITE_API_URL=https://api.junyue.app/api

# 应用配置
VITE_APP_NAME=君悦彩虹SPA
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=专业的SPA预约服务平台

# 域名配置
VITE_DOMAIN=junyue.app
VITE_APP_URL=https://junyue.app

# 支付配置
VITE_PAYMENT_PROVIDER=yungou
VITE_PAYMENT_ENV=production

# 地图配置（如果使用）
VITE_MAP_KEY=your_map_api_key

# 分析配置（如果使用）
VITE_GA_ID=your_google_analytics_id

# 错误监控（如果使用）
VITE_SENTRY_DSN=your_sentry_dsn

# 功能开关
VITE_ENABLE_CONSOLE_LOG=false
VITE_ENABLE_DEBUG=false
VITE_ENABLE_MOCK=false

# CDN配置（如果使用）
VITE_CDN_URL=https://cdn.junyue.app

# 文件上传配置
VITE_UPLOAD_MAX_SIZE=10485760
VITE_UPLOAD_ALLOWED_TYPES=jpg,jpeg,png,gif,webp
